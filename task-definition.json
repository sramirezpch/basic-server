{
    "family": "basic-server-family",
    "containerDefinitions": [
      {
        "name": "${container-name}",
        "image": "${container-image}",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": "${container-port}",
            "hostPort": "${container-port}"
          }
        ]
      }
    ]
  }
  